# login1
Host *.ssk.ynwp.yahoo.co.jp
    ProxyCommand ssh login1.corp.yahoo.co.jp nc %h %p
    ControlMaster auto
    ControlPath ~/.ssh/ctrl-%r@%h

Host *.kks.ynwp.yahoo.co.jp
    ProxyCommand ssh login1.corp.yahoo.co.jp nc %h %p
    ControlMaster auto
    ControlPath ~/.ssh/ctrl-%r@%h

# other
Host github.corp.yahoo.co.jp
    User motani
    Port 22
    Hostname github.corp.yahoo.co.jp

# All config
Host *
  User         motani
  ForwardAgent yes
  Protocol     2
